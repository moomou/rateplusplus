    <div class="container-narrow">
      <a href="#"><i data-toggle="tooltip" title="Fork Ranking" id="rankingFork" class="fa fa-code-fork fa-2x btn-like"> </i></a>
      <i data-toggle="tooltip" title="Private Ranking" id="rankingPrivacy" class="fa fa-lock fa-2x"> </i>
      <span contenteditable="true" id="rankingName"> </span>
