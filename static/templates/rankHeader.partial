        <i id="rankingViewCancelBtn" style="float: right; margin-top: -7px;" class="fa fa-times-circle fa-2x"></i>
        <a target="_blank" id="rankingHeaderShareTwitterBtn" href="#"><i style="float: right; margin: -7px 5px 0 0;" class="fa fa-twitter-square fa-2x"></i></a>
      </div>
    </div>
  </div>
